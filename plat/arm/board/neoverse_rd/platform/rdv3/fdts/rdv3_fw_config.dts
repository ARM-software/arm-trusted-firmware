#include <common/tbbr/tbbr_img_def.h>

/dts-v1/;

/ {
	dtb-registry {
		compatible = "fconf,dyn_cfg-dtb_registry";

		tb_fw-config {
			load-address = <0x0 0x01f300>;
			max-size = <0x200>;
			id = <TB_FW_CONFIG_ID>;
		};

		nt_fw-config {
			load-address = <0x0 0xF3000000>;
			max-size = <0x0100000>;
			id = <NT_FW_CONFIG_ID>;
		};
	};
};
